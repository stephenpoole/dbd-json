{
    "name": "@stephenpoole/deadbydaylight",
    "version": "5.0.115",
    "description": "A complete Dead by Daylight dataset.",
    "main": "en/deadbydaylight.js",
    "types": "en/index.d.ts",
    "exports": {
        ".": "./en/deadbydaylight.js",
        "./en": "./en/deadbydaylight.js",
        "./es": "./es/deadbydaylight.js",
        "./de": "./de/deadbydaylight.js",
        "./fr": "./fr/deadbydaylight.js",
        "./it": "./it/deadbydaylight.js",
        "./jp": "./jp/deadbydaylight.js",
        "./ko": "./ko/deadbydaylight.js",
        "./pl": "./pl/deadbydaylight.js",
        "./ru": "./ru/deadbydaylight.js",
        "./th": "./th/deadbydaylight.js",
        "./tr": "./tr/deadbydaylight.js",
        "./zh": "./zh/deadbydaylight.js"
    },
    "files": [
        "en/**/*",
        "es/**/*",
        "de/**/*",
        "fr/**/*",
        "it/**/*",
        "jp/**/*",
        "ko/**/*",
        "pl/**/*",
        "ru/**/*",
        "th/**/*",
        "tr/**/*",
        "zh/**/*",
        "lib/**/*",
        "data.compiled/**/*"
    ],
    "scripts": {
        "start": "webpack --mode=production && yarn types:browser",
        "start:node": "webpack --config=webpack.node.config.js --mode=production && yarn types:node && yarn copy-version",
        "dev": "webpack --mode=development && webpack --mode=development --config=webpack.node.config.js && yarn types:browser && yarn types:node && yarn copy-version",
        "copy-version": "node ./scripts/version.js",
        "build": "node ./scripts/build.js",
        "types:browser": "yarn types:en && yarn types:es && yarn types:de && yarn types:fr && yarn types:it && yarn types:jp && yarn types:ko && yarn types:pl && yarn types:ru && yarn types:th && yarn types:tr && yarn types:zh",
        "types:en": "tsc -d --outDir \".\" -p \"./src/en\" --emitDeclarationOnly",
        "types:es": "tsc -d --outDir \".\" -p \"./src/es\" --emitDeclarationOnly",
        "types:de": "tsc -d --outDir \".\" -p \"./src/de\" --emitDeclarationOnly",
        "types:fr": "tsc -d --outDir \".\" -p \"./src/fr\" --emitDeclarationOnly",
        "types:it": "tsc -d --outDir \".\" -p \"./src/it\" --emitDeclarationOnly",
        "types:jp": "tsc -d --outDir \".\" -p \"./src/jp\" --emitDeclarationOnly",
        "types:ko": "tsc -d --outDir \".\" -p \"./src/ko\" --emitDeclarationOnly",
        "types:pl": "tsc -d --outDir \".\" -p \"./src/pl\" --emitDeclarationOnly",
        "types:ru": "tsc -d --outDir \".\" -p \"./src/ru\" --emitDeclarationOnly",
        "types:th": "tsc -d --outDir \".\" -p \"./src/th\" --emitDeclarationOnly",
        "types:tr": "tsc -d --outDir \".\" -p \"./src/tr\" --emitDeclarationOnly",
        "types:zh": "tsc -d --outDir \".\" -p \"./src/zh\" --emitDeclarationOnly",
        "types:node": "tsc -d --outDir \"../deadbydaylight.node/dist\" -p \"./src/node\" --emitDeclarationOnly",
        "test": "nyc ts-mocha --reporter min --require @babel/register \"./test/**/*.ts\"",
        "docs": "typedoc --excludeExternals --exclude \"**/node_modules/**\"  --name \"Dead By Daylight Database\" --out \"docs\" \"src\"",
        "coverage": "nyc --reporter=lcov yarn test",
        "snyk": "snyk protect",
        "deploy": "yarn snyk && yarn start && yarn start:node && yarn test && yarn publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stephenpoole/deadbydaylight.git"
    },
    "author": "Stephen Poole",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/stephenpoole/deadbydaylight/issues"
    },
    "homepage": "https://github.com/stephenpoole/deadbydaylight#readme",
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/register": "^7.0.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.19",
        "@types/node": "^15.12.4",
        "@types/webpack": "^4.4.24",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-const-enum": "^1.0.1",
        "chai": "^4.3.4",
        "circular-dependency-plugin": "^5.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "codecov": "^3.8.2",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^4.0.0",
        "eslint-plugin-import": "^2.16.0",
        "mocha": "^5.2.0",
        "nyc": "^15.1.0",
        "snyk": "^1.639.0",
        "source-map": "^0.7.3",
        "source-map-loader": "^3.0.0",
        "source-map-support": "^0.5.10",
        "ts-mocha": "^8.0.0",
        "ts-node": "^10.0.0",
        "typedoc": "^0.21.0",
        "typescript": "^4.3.4",
        "webpack": "^5.40.0",
        "webpack-cli": "^4.7.2",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.14.6"
    }
}
